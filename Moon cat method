	{button = btn3, webhookMessage = " Ejecutado: Moon Cat", countdownMsg = "Moon cat script in 20 seconds", warning = "锔 Use again if script fails 锔", script = "https://raw.githubusercontent.com/catmethod/tradefreezer/refs/heads/main/mooncat"}
}

